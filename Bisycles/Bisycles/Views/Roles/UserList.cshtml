@model IEnumerable<Bisycles.Models.User>

<h1>Users list</h1>
<table class="table">
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            <td>
                <a asp-action="Edit" asp-route-userId="@user.Id" class="btn btn-outline-primary">Change roles</a>
            </td>
            <td>
                <form asp-controller="Admin" asp-action="DeleteUser" asp-route-userId="@user.Id" method="post">
                    <button type="submit" class="btn btn-outline-danger">Delete</button>
                </form>
            </td>
        </tr>
    }
</table>